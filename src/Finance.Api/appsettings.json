{
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=dashfin;Username=postgres;Password=postgres"
  },
  "Jwt": {
    "Issuer": "dashfin",
    "Audience": "dashfin",
    "SigningKey": "CHANGE_ME_DEV_ONLY",
    "AccessTokenMinutes": 15
  },
  "FileStorage": {
    "Local": {
      "RootPath": "var/files"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}

